\GenomeClass::evaluator : // Returns the score as a real value
  // uses Genome to evaluate the quality of the individual
  if (objective_ == nullptr)
	  objective_ = new double[getNumberOfObjectives()];
    int k = NB_VARIABLES - NB_OBJECTIVES + 1;
    //double * x = new double[NB_VARIABLES];

//    for (int i = 0; i < NB_VARIABLES; i++)
//	y[i] = getIndividualVariables()[i]->getValue();
    double g = 0.0;
    for ( int i = NB_VARIABLES - k; i < NB_VARIABLES; i++)
	g += getIndividualVariables()[i]->getValue();
    g = 1 + (9.0 * g)/k;
    for (int i = 0; i < NB_OBJECTIVES-1; i++)
	objective_[i] = getIndividualVariables()[i]->getValue();
    double h =0.0;
    for ( int i = 0; i < NB_OBJECTIVES-1; i++)
	h+=(objective_[i]/(1.0+g))*(1+sin(3.0*PI*objective_[i]));
    h = NB_OBJECTIVES - h;
    objective_[NB_OBJECTIVES - 1] = (1+g)*h;

    
  return 0.0;
\end

